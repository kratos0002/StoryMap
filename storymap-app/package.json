{
  "name": "react_repo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "collect:test": "node scripts/test_hyperbrowser_connection.js",
    "collect:stories": "node scripts/run_hyperbrowser_collection.js",
    "collect:validate": "node scripts/validate_stories.js",
    "gutendx:test": "node scripts/run_gutendx_collection.js --dry-run",
    "gutendx:collect": "node scripts/run_gutendx_collection.js",
    "gutendx:collect:small": "node scripts/run_gutendx_collection.js --max-stories=25",
    "gutendx:collect:large": "node scripts/run_gutendx_collection.js --max-stories=200",
    "gutendx:mystery": "node scripts/run_gutendx_collection.js --topics='mystery,detective,crime' --max-stories=50",
    "gutendx:horror": "node scripts/run_gutendx_collection.js --topics='horror,ghost stories,supernatural' --max-stories=50",
    "gnikdroy:test": "node scripts/run_gnikdroy_collection.js --dry-run",
    "gnikdroy:collect": "node scripts/run_gnikdroy_collection.js",
    "gnikdroy:collect:small": "node scripts/run_gnikdroy_collection.js --max-stories=25",
    "gnikdroy:collect:large": "node scripts/run_gnikdroy_collection.js --max-stories=200",
    "gnikdroy:mystery": "node scripts/run_gnikdroy_collection.js --max-stories=50",
    "gnikdroy:horror": "node scripts/run_gnikdroy_collection.js --max-stories=50",
    "gnikdroy:classic": "node scripts/run_gnikdroy_collection.js --max-stories=75",
    "test:single-story": "node scripts/test_single_story.js",
    "mcp:test": "node scripts/test-mcp-server.js",
    "mcp:start": "node mcp-server/supabase-server.js",
    "mcp:dev": "nodemon mcp-server/supabase-server.js",
    "enrich:analyze": "node scripts/story_enrichment_pipeline.js --dry-run",
    "enrich:single": "node scripts/story_enrichment_pipeline.js --batch-size=1",
    "enrich:batch": "node scripts/story_enrichment_pipeline.js --batch-size=5",
    "enrich:batch:large": "node scripts/story_enrichment_pipeline.js --batch-size=10",
    "enrich:all": "node scripts/story_enrichment_pipeline.js",
    "mcp:enhanced": "node mcp-server/enhanced-supabase-server.js",
    "enrich:test": "node -e \"import('./scripts/story_enrichment_pipeline.js').then(m => new m.default().analyzeStoriesForEnrichment().then(console.log))\"",
    "test:enrichment": "node scripts/test_enrichment_system.js",
    "db:check": "node scripts/database-integrity-checker.js",
    "db:fix": "node scripts/database-integrity-checker.js --auto-fix",
    "db:health": "npm run db:check && npm run enrich:analyze",
    "maintenance": "npm run db:check && npm run enrich:analyze && echo 'âœ… Maintenance complete'"
  },
